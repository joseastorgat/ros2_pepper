#!/bin/bash
set -euf -o pipefail
mkdir -p /home/nao/ros_dependencies_sources/build/SDL_image
export PATH=$PATH:/home/nao/${INSTALL_ROOT}/ros_dependencies/bin
cd /home/nao/ros_dependencies_sources/build/SDL_image
../../src/SDL_image/configure \
  --prefix=/home/nao/${INSTALL_ROOT}/ros_dependencies \
  --host=i686-aldebaran-linux-gnu \
  --build=x86_64-linux \
  --enable-shared
make -j4 install
